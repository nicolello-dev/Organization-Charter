// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Employee {
  id              String     @id @default(uuid())
  team_member     String
  competence      String
  team            String
  area            String
  function        String
  tribe           String
  competence_lead String
  team_lead       String
  area_lead       String
  function_lead   String
  tribe_lead      String
  datapoint       Datapoint? @relation(fields: [datapointId], references: [id])
  datapointId     String?
}

model Datapoint {
  id        String     @id @default(uuid())
  createdAt DateTime   @default(now())
  employees Employee[]
}
